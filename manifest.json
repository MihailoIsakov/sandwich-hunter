{
"name":"Lovac na sendviče",
"description":"Ocenite koliko je komentar na Blicu zaslužio sendviča!",
"version":"0.1",
"manifest_version":2,
"icons": { 
    "48": "sandwich48.png",
    "128": "sandwich128.png"
},
"permissions": [ 
    "http://0.0.0.0:5000/*",
    "storage",
    "http://www.blic.rs/*",
    "https://www.blic.rs/*"
],
"content_scripts": [
    {
      "matches": ["http://www.blic.rs/*","https://www.blic.rs/*"],
      "js": ["jquery-1.12.1.min.js", "myscript.js"],
      "css": ["bot.css"]
    }
  ],
"background": {
    "scripts": ["background.js"]
    }
}

