{
"name":"Sendvič hunter",
"description":"Ocenite koliko je komentar zaslužio sendviča!",
"version":"0.1",
"manifest_version":2,
"permissions": [ 
    "storage",
    "http://www.blic.rs/*",
    "https://www.blic.rs/*"
],
"content_scripts": [
    {
      "matches": ["http://www.blic.rs/*","https://www.blic.rs/*"],
      "js": ["jquery-1.12.1.min.js", "myscript.js"],
      "css": ["bot.css"]
    }
  ],
"background": {
    "scripts": ["background.js"]
    }
}

